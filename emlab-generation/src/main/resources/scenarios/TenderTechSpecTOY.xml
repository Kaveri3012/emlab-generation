<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean post processor to persist created beans -->
	<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />


	<!-- Decarbonization Model -->
	<bean id="decarbonizationModel" class="emlab.gen.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="40"
		p:co2TradingImplemented="false" p:realRenewableDataImplemented="true"
		p:co2BankingIsImplemented="false"
		p:deletionOldPPDPBidsAndCashFlowsEnabled="true" p:deletionAge="4"
		p:centralForecastBacklookingYears="5"
		p:centralForecastingYear="3" p:centralCO2TargetReversionSpeedFactor="3"
		p:centralPrivateDiscountingRate="0.05" p:centralCO2BackSmoothingFactor="0"
		p:stabilityReserveIsActive="false"
		p:stabilityReserveFirstYearOfOperation="100"
		p:stabilityReserveBankingFirstYear="0.8"
		p:stabilityReserveBankingSecondYear="0.5"
		p:stabilityReserveBankingThirdYear="0.2"
		p:feedInPremiumImplemented="false"
		p:renewableTenderSchemeImplemented="true">
	</bean>

	<!-- Interconnector -->
	<!-- https://www.rvo.nl/sites/default/files/2015/05/IP01_Inpassingsplan%20DW380%20Toelichting%20en%20regels%2020150415%20-%20EXCL%20BIJLAGEN_vKT.pdf
		says 2450 MW in (2014) with an expected increase to 3950 MW in 2017 -->
	<bean id="interconnectorNetherlandsGermany" class="emlab.gen.domain.technology.Interconnector"
		p:capacity="0">
		<property name="connections">
			<set>
				<ref bean="nlNode1" />
				<ref bean="deNode1" />
			</set>
		</property>
	</bean>

	<!-- Regulator NL -->
<bean id="regulatorNL" class="emlab.gen.domain.agent.Regulator"
		p:name="regulatorNetherlands" p:numberOfYearsLookingBackToForecastDemand="4">
 		<property name="zone" ref="nl" />
 	</bean>
	<!-- Renewable Tender Scheme -->

		<bean id="renewableSupportSchemeNLphotovoltaicsPGT"
		class="emlab.gen.domain.policy.renewablesupport.RenewableSupportSchemeTender"
		p:name="TenderPv"
		p:futureTenderOperationStartTime="5"
		p:supportSchemeDuration="10"
		p:technologySpecificityEnabled="true"
		p:expostRevenueCalculation="true">
		<property name="regulator" ref="regulatorNL" />
		<property name="powerGeneratingTechnologiesEligible">
			<set>
				<ref bean="photovoltaicPGT" />
			</set>
		</property>
	</bean>


		<bean id="renewableSupportSchemeNLwindPGT"
		class="emlab.gen.domain.policy.renewablesupport.RenewableSupportSchemeTender"
		p:name="TenderWind"
		p:futureTenderOperationStartTime="5"
		p:supportSchemeDuration="10"
		p:technologySpecificityEnabled="true"
		p:expostRevenueCalculation="true">
		<property name="regulator" ref="regulatorNL" />
		<property name="powerGeneratingTechnologiesEligible">
			<set>
				<ref bean="windPGT" />
			</set>
		</property>
	</bean>



	<!-- Renewable Energy Target NL -->
	<bean id="renewableTenderTarget"
		class="emlab.gen.domain.policy.renewablesupport.RenewableTarget"
		p:yearlyRenewableTargetTimeSeries-ref="nl_nreap"
		p:targetTechnologySpecific="false">
		<property name="regulator" ref="regulatorNL" />
	</bean>

	<bean id="nl_nreap" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/nodeAndTechSpecificPotentials.csv" p:startingYear="0"
		p:delimiter="," p:variableName="nl_dummy" />

		<!-- Technology Specific Renewable Energy Targets NL -->

	<bean id="renewableTenderTargetNLphotovoltaicPGT"
		class="emlab.gen.domain.policy.renewablesupport.RenewableTarget"
		p:yearlyRenewableTargetTimeSeries-ref="nl_nreap_photovoltaicPGT"
		p:targetTechnologySpecific = "true">
		<property name="regulator" ref="regulatorNL" />
		<property name="powerGeneratingTechnology" ref="photovoltaicPGT" />
	</bean>

	<bean id="nl_nreap_photovoltaicPGT" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/nodeAndTechSpecificPotentialsDummy.csv" p:startingYear="0"
		p:delimiter="," p:variableName="nl_dummyTargetPV" />


		<bean id="renewableTenderTargetNLwindPGT"
		class="emlab.gen.domain.policy.renewablesupport.RenewableTarget"
		p:yearlyRenewableTargetTimeSeries-ref="nl_nreap_windPGT"
		p:targetTechnologySpecific = "true">
		<property name="regulator" ref="regulatorNL" />
		<property name="powerGeneratingTechnology" ref="windPGT" />
	</bean>

	<bean id="nl_nreap_windPGT" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/nodeAndTechSpecificPotentialsDummy.csv" p:startingYear="0"
		p:delimiter="," p:variableName="nl_dummyTargetWind" />




	<!-- Substances -->

	<bean id="naturalgas" class="emlab.gen.domain.technology.Substance"
		p:name="Natural Gas" p:quality="1" p:energyDensity="1000"
		p:co2Density="0.05194" p:unit="GJ" />

	<bean id="electricity" class="emlab.gen.domain.technology.Substance"
		p:name="Electricity" p:quality="1" p:energyDensity="0" p:co2Density="0"
		p:unit="MWh_el" />
	<bean id="co2" class="emlab.gen.domain.technology.Substance"
		p:name="CO2" p:quality="1" p:energyDensity="0" p:co2Density="1"
		p:unit="tCO2" />



	<!-- Zones -->
	<bean id="nl" class="emlab.gen.domain.gis.Zone" p:name="Country A" />
	<bean id="de" class="emlab.gen.domain.gis.Zone" p:name="Country B" />



	<!-- PowerGrid -->
	<bean id="nlNode1" class="emlab.gen.domain.technology.PowerGridNode"
		p:capacityMultiplicationFactor="1.0" p:name="nlNode1">
		<property name="zone" ref="nl" />
		<property name="hourlyDemand" ref="demandNL" />
	</bean>




	<bean id="deNode1" class="emlab.gen.domain.technology.PowerGridNode"
		p:capacityMultiplicationFactor="2.0" p:name="deNode1">
		<property name="zone" ref="de" />
		<property name="hourlyDemand" ref="demandDE" />
	</bean>


	<!-- Demand in MW -->
	<bean id="demandNL" class="emlab.gen.trend.HourlyCSVTimeSeries"
		p:filename="/data/Load_NL_2010.csv" p:variableName="nl_demand"
		p:timeSeriesAreInDifferentColumns="true" p:delimiter="," />

	<bean id="demandDE" class="emlab.gen.trend.HourlyCSVTimeSeries"
		p:filename="/data/Load_DE_2010.csv" p:variableName="de_demand"
		p:timeSeriesAreInDifferentColumns="true" p:delimiter="," />

	<!-- Demand growth 
		<bean id="demandGrowthTrendNL" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/stochasticDemandNLandDE/demandGrowthNL.csv"
		p:startingYear="0" p:delimiter="," p:variableName="demandNL-1" />
		
			<bean id="demandGrowthTrendDE" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/stochasticDemandNLandDE/demandGrowthDE.csv"
		p:startingYear="0" p:delimiter="," p:variableName="demandDE-1" /> -->
		
	<bean class="emlab.gen.trend.TriangularTrend" id="demandGrowthTrendNL"
		p:start="1" p:min="1" p:max="1"
		p:top="1" />

	<bean class="emlab.gen.trend.TriangularTrend" id="demandGrowthTrendDE"
		p:start="1" p:min="1" p:max="1"
		p:top="1" />



	<!-- Intermittent Production Profiles -->
	<!-- Photovoltaic nodes Netherlands (1-3 nodes) -->
	<bean id="photovoltaicNodeNL1" class="emlab.gen.domain.technology.IntermittentResourceProfile"
		p:filename="/data/multiNodeProduction_NL_solarPV.csv"
		p:timeSeriesAreInDifferentColumns="true" p:delimiter=","
		p:variableName="nl_node_1">
		<property name="intermittentTechnology" ref="photovoltaicPGT"></property>
		<property name="intermittentProductionNode" ref="nlNode1"></property>
	</bean>



	<!-- Wind onshore nodes Netherlands (1-3 nodes) -->
	<bean id="windOnSpeedNodeNL1" class="emlab.gen.domain.technology.IntermittentResourceProfile"
		p:filename="/data/multiNodeProduction_NL_windOn.csv"
		p:timeSeriesAreInDifferentColumns="true" p:delimiter=","
		p:variableName="nl_node_1">
		<property name="intermittentTechnology" ref="windPGT"></property>
		<property name="intermittentProductionNode" ref="nlNode1"></property>
	</bean>







<!-- Segments of load duration curves -->
	<bean id="segment1" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="20"
		p:segmentID="3" />
	<bean id="segment2" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="30"
		p:segmentID="2" />
	<bean id="segment3" class="emlab.gen.domain.market.electricity.Segment" p:lengthInHours="50"
		p:segmentID="1" />


		<!-- Markets -->
	<!-- Electricity Spot Country A -->
	<bean id="NetherlandsElectricitySpotMarket"
		class="emlab.gen.domain.market.electricity.ElectricitySpotMarket"
		p:name="Country A electricity spot market" p:auction="false"
		p:referencePrice="40" p:valueOfLostLoad="2000" p:lookback="5"
		p:backlookingForDemandForecastinginDismantling="5">
		<property name="substance" ref="electricity" />
		<property name="demandGrowthTrend" ref="demandGrowthTrendNL" />
		<property name="zone" ref="nl" />

		<property name="loadDurationCurve">
			<set>
				<ref bean="segment1nl" />
				<ref bean="segment2nl" />
				<ref bean="segment3nl" />

			</set>

		</property>

	</bean>

	<bean id="segment1nl" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="50">
		<property name="segment" ref="segment1" />
	</bean>
	<bean id="segment2nl" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="500">
		<property name="segment" ref="segment2" />
	</bean>
	<bean id="segment3nl" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="5000">
		<property name="segment" ref="segment3" />
	</bean>


	<!-- Electricity Spot Country B -->
	<bean id="GermanyElectricitySpotMarket"
		class="emlab.gen.domain.market.electricity.ElectricitySpotMarket"
		p:name="Country B electricity spot market" p:auction="false"
		p:referencePrice="40" p:valueOfLostLoad="2000" p:lookback="5"
		p:backlookingForDemandForecastinginDismantling="5">
		<property name="substance" ref="electricity" />
		<property name="demandGrowthTrend" ref="demandGrowthTrendDE" />
		<property name="zone" ref="de" />

		<property name="loadDurationCurve">
			<set>
				<ref bean="segment1de" />
				<ref bean="segment2de" />
				<ref bean="segment3de" />

			</set>

		</property>

	</bean>

	<bean id="segment1de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="50">
		<property name="segment" ref="segment1" />
	</bean>
	<bean id="segment2de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="500">
		<property name="segment" ref="segment2" />
	</bean>
	<bean id="segment3de" class="emlab.gen.domain.market.electricity.SegmentLoad"
		p:baseLoad="5000">
		<property name="segment" ref="segment3" />
	</bean>


	<!-- Commodity Markets -->
	<bean id="naturalgasMarket" class="emlab.gen.domain.market.CommodityMarket"
		p:auction="false" p:name="Natural gas market">
		<property name="substance" ref="naturalgas" />
	</bean>


	<!-- CO2 Auction -->
	<bean id="co2Auction" class="emlab.gen.domain.market.CO2Auction"
		p:auction="false" p:referencePrice="0" p:name="CO2 auction"
		p:substance-ref="co2" />


	<!-- Fuel Prices -->
	<bean id="gasPrice" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/stochasticFuelPrices/gasPrice.csv" p:startingYear="0"
		p:delimiter="," p:variableName="gasPrice-1" />



	<!-- Agents -->
	<bean id="gasSupplier" class="emlab.gen.domain.agent.CommoditySupplier">
		<property name="name" value="Gas supplier" />
		<property name="substance" ref="naturalgas" />
		<property name="priceOfCommodity" ref="gasPrice" />
	</bean>





	<bean id="energyConsumer" class="emlab.gen.domain.agent.EnergyConsumer"
		p:ltcMaximumCoverageFraction="0.8" p:contractDurationPreferenceFactor=".03"
		p:contractWillingnessToPayFactor="1.2">
		<property name="name" value="Energy consumer" />
	</bean>

	<bean id="energyProducerA" class="emlab.gen.domain.agent.EnergyProducer"
		p:name="Energy Producer A" p:cash="${startCash}" p:investmentRole-ref="standardInvestmentRole"
		p:longTermContractMargin="0.1" p:longTermContractPastTimeHorizon="3"
		p:investmentFutureTimeHorizon="${investmentFutureTimeHorizon}"
		p:pastTimeHorizon="5" p:equityInterestRate="0.12" p:loanInterestRate="0.09"
		p:debtRatioOfInvestments="0.7"
		p:dismantlingProlongingYearsAfterTechnicalLifetime="0"
		p:dismantlingRequiredOperatingProfit="0" p:downpaymentFractionOfCash="0.5"
		p:willingToInvest="true" p:priceMarkUp="${priceMarkUp}"
		p:numberOfYearsBacklookingForForecasting="${numberOfYearsBacklookingForForecasting}"
		p:investorMarket-ref="NetherlandsElectricitySpotMarket" />


	<bean id="standardInvestmentRole"
		class="emlab.gen.role.investment.InvestInPowerGenerationTechnologiesStandard" />


	<!-- Initial portfolios You still have to check the initial fractions -->

	<bean id="powerPlantManufacturer" class="emlab.gen.domain.agent.PowerPlantManufacturer"
		p:name="Power Plant Manufacturer" />
	<bean id="bigBank" class="emlab.gen.domain.agent.BigBank" p:name="Big Bank" />
	<bean id="powerPlantMaintainer" class="emlab.gen.domain.agent.PowerPlantMaintainer"
		p:name="Power Plant Maintainer" />

	<!-- Power plant Factory -->
	<bean id="NLPortfolio" class="emlab.gen.domain.factory.ElectricityProducerFactory">
	<property name="capacityMargin" value="1.0" />
		<property name="portfolioShares">
			<map>

	<entry>
		<key>
			<ref bean="CCGT" />
		</key>
		<value>0.3</value>
	</entry>
	
	<entry>
		<key>
			<ref bean="windPGT" />
		</key>
		<value>0.35</value>
	</entry>
	<entry>
		<key>
			<ref bean="photovoltaicPGT" />
		</key>
		<value>0.35</value>
	</entry>
</map>
</property>
		<property name="nodes">
			<set>
				<ref bean="nlNode1" />

			</set>
		</property>
		<property name="market" ref="NetherlandsElectricitySpotMarket" />
		<property name="producers">
			<list>
				<ref bean="energyProducerA" />
			</list>
		</property>
	</bean>

	<!-- CO2 Tax -->
	<bean id="co2TaxTrend" class="emlab.gen.trend.StepTrend" p:start="${co2TaxStart}"
		p:duration="${co2TaxDuration}" p:increment="${co2TaxIncrement}"
		p:minValue="0" />

	<!-- CO2 Cap -->
	<bean id="co2CapTrend" class="emlab.gen.trend.StepTrend" p:start="${co2CapStart}"
		p:duration="${co2CapDuration}" p:increment="${co2CapIncrement}"
		p:minValue="0" />

	<!-- Minimum CO2 Price Trends -->
	<bean id="minCo2PriceTrend" class="emlab.gen.trend.StepTrend"
		p:start="${minCo2PriceStart}" p:duration="${minCo2PriceDuration}"
		p:increment="${minCo2PriceIncrement}" p:minValue="0" />

	<bean id="minCo2PriceTrendNL" class="emlab.gen.trend.StepTrend"
		p:start="0" p:duration="1" p:increment="0" p:minValue="0" />


	<!-- Government -->
	<bean id="government" class="emlab.gen.domain.agent.Government"
		p:name="European Government" p:co2Penalty="500">
		<property name="co2TaxTrend" ref="co2TaxTrend" />
		<property name="co2CapTrend" ref="co2CapTrend" />
		<property name="minCo2PriceTrend" ref="minCo2PriceTrend" />
	</bean>

	<bean id="governmentNL" class="emlab.gen.domain.agent.NationalGovernment"
		p:name="Country A Government">
		<property name="minNationalCo2PriceTrend" ref="minCo2PriceTrendNL" />
		<property name="governedZone" ref="nl" />
	</bean>

	<bean id="governmentDE" class="emlab.gen.domain.agent.NationalGovernment"
		p:name="Country B Government">
		<property name="minNationalCo2PriceTrend" ref="minCo2PriceTrendNL" />
		<property name="governedZone" ref="de" />
	</bean>


	<!-- Power Generation Technologies -->



	<bean id="CCGT" class="emlab.gen.domain.technology.PowerGeneratingTechnology"
		p:name="CCGT" p:capacity="776" p:investmentCostTimeSeries-ref="ccgtInvestmentCostTimeSeries"
		p:fixedOperatingCostTimeSeries-ref="ccgtFixedOperatingCostTimeSeries"
		p:efficiencyTimeSeries-ref="ccgtEfficiencyTimeSeries"
		p:co2CaptureEffciency="0" p:depreciationTime="15"
		p:minimumRunningHours="0" p:fixedOperatingCostModifierAfterLifetime=".05"
		p:expectedLifetime="40" p:expectedLeadtime="2" p:expectedPermittime="1"
		p:minimumFuelQuality="1" p:maximumInstalledCapacityFractionInCountry="1"
		p:maximumInstalledCapacityFractionPerAgent="1"
		p:baseSegmentDependentAvailability="1"
		p:peakSegmentDependentAvailability="1"
		p:applicableForLongTermContract="true" p:intermittent="false">
		<property name="fuels">
			<set>
				<ref bean="naturalgas" />
			</set>
		</property>
	</bean>

	<bean id="ccgtInvestmentCostTimeSeries" class="emlab.gen.trend.GeometricTrend"
		p:growthRate="-0.0075679493" p:start="646830" />
	<bean id="ccgtFixedOperatingCostTimeSeries" class="emlab.gen.trend.GeometricTrend"
		p:growthRate="0" p:start="29470" />
	<bean id="ccgtEfficiencyTimeSeries" class="emlab.gen.trend.GeometricTrend"
		p:growthRate="0.002068676" p:start=".59" />


	<bean id="windPGT" class="emlab.gen.domain.technology.PowerGeneratingTechnology"
		p:name="Wind" p:capacity="600" p:investmentCostTimeSeries-ref="windInvestmentCostTimeSeries"
		p:fixedOperatingCostTimeSeries-ref="windFixedOperatingCostTimeSeries"
		p:efficiencyTimeSeries-ref="windEfficiencyTimeSeries"
		p:co2CaptureEffciency="0" p:depreciationTime="15"
		p:minimumRunningHours="0" p:fixedOperatingCostModifierAfterLifetime=".05"
		p:expectedLifetime="25" p:expectedLeadtime="1" p:expectedPermittime="1"
		p:minimumFuelQuality="1" p:maximumInstalledCapacityFractionInCountry="1"
		p:maximumInstalledCapacityFractionPerAgent="1"
		p:baseSegmentDependentAvailability="0.40"
		p:peakSegmentDependentAvailability="0.05" p:intermittent="true"
		p:applicableForLongTermContract="true">
	</bean>

	<bean id="windInvestmentCostTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="Wind_Inv" />
	<bean id="windFixedOperatingCostTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="Wind_OM" />
	<bean id="windEfficiencyTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="Wind_Eff" />




	<bean id="photovoltaicPGT" class="emlab.gen.domain.technology.PowerGeneratingTechnology"
		p:name="Photovoltaic" p:capacity="500" p:investmentCostTimeSeries-ref="pvInvestmentCostTimeSeries"
		p:fixedOperatingCostTimeSeries-ref="pvFixedOperatingCostTimeSeries"
		p:efficiencyTimeSeries-ref="pvEfficiencyTimeSeries"
		p:co2CaptureEffciency="0" p:depreciationTime="15"
		p:minimumRunningHours="0" p:fixedOperatingCostModifierAfterLifetime=".05"
		p:expectedLifetime="25" p:expectedLeadtime="1" p:expectedPermittime="0"
		p:minimumFuelQuality="1" p:maximumInstalledCapacityFractionInCountry="1"
		p:maximumInstalledCapacityFractionPerAgent="1"
		p:baseSegmentDependentAvailability="0.20"
		p:peakSegmentDependentAvailability="0.04"
		p:applicableForLongTermContract="true" p:intermittent="true">
	</bean>


	<bean id="pvInvestmentCostTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="PV_Inv" />
	<bean id="pvFixedOperatingCostTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="PV_OM" />
	<bean id="pvEfficiencyTimeSeries" class="emlab.gen.trend.TimeSeriesCSVReader"
		p:filename="/data/learningCurves.csv" p:startingYear="-50"
		p:delimiter="," p:variableName="PV_Eff" />



	<!-- Parameter file -->
	<bean id="projectPropertyConfigurer"
		class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
		<property name="location"
			value="classpath:scenarios/RDJ-RT-TC-ThreeMultiNodes.properties" />
	</bean>


</beans>
