<beans 
	xmlns = "http://www.springframework.org/schema/beans"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:p = "http://www.springframework.org/schema/p"
	xsi:schemaLocation = "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<!-- Bean post processor to persist created beans -->
<bean class="agentspring.graphdb.PersistingBeanPostProcessor" />
	
<!-- Variables used more than once in the configuration file can be centrally placed here -->

<!-- Decarbonization Model -->
<bean id = "decarbonizationModel" class = "emlab.gen.domain.agent.DecarbonizationModel"
		p:name="Decarbonization Model" p:simulationLength="${simulationLenght}"
		p:co2TradingImplemented="false"
		p:realRenewableDataImplemented="true"
		p:iterationSpeedFactor="3" p:iterationSpeedCriterion="0.005"
		p:capDeviationCriterion="0.03" p:deletionOldPPDPBidsAndCashFlowsEnabled="true"
		p:deletionAge="2">
</bean>

<!-- Capacity Market -->

<!-- Demand Growth -->

<bean class = "emlab.gen.trend.TriangularTrend" id = "demandGrowthTrendNL"
		p:start="${demandGrowthNLStart}" p:min="${demandGrowthNLMin}" p:max="${demandGrowthNLMax}"
		p:top="${demandGrowthNLTop}" />
		
<!-- Substances -->
<bean id = "coal" class = "emlab.gen.domain.technology.Substance" p:name="Coal"
	p:quality="1" p:energyDensity="29308" p:co2Density="2.78426" p:unit="tSKE" />
<bean id = "lignite" class = "emlab.gen.domain.technology.Substance" p:name="Lignite"
	p:quality="1" p:energyDensity="3600" p:co2Density="0.41" p:unit="MWh_th" />
<bean id = "naturalgas" class = "emlab.gen.domain.technology.Substance" p:name="Natural Gas"
	p:quality="1" p:energyDensity="36" p:co2Density="0.00187" />
<bean id = "biomass" class = "emlab.gen.domain.technology.Substance" p:name="biomass"
	p:quality="0.5" p:energyDensity="25000" p:co2Density="0" />
<bean id = "uranium" class = "emlab.gen.domain.technology.Substance" p:name="Uranium"
	p:quality="1" p:energyDensity="3888000000" p:co2Density="0" />
<bean id = "electricity" class = "emlab.gen.domain.technology.Substance" p:name="Electricity"
	p:quality="1" p:energyDensity="0" p:co2Density="0" />
<bean id = "co2" class = "emlab.gen.domain.technology.Substance" p:name="CO2"
	p:quality="1" p:energyDensity="0" p:co2Density="1" />

<!-- Zones -->
<bean id = "nl" class = "emlab.gen.domain.gis.Zone" p:name="Country A" />

<!-- PowerGrid -->
<bean id = "beneluxNode" class = "emlab.gen.domain.technology.PowerGridNode"	
	p:capacityMultiplicationFactor="1.0">
	<property name="zone" ref="nl" />
	<property name="hourlyDemand" ref="demandNL" />
</bean>

<bean id = "interconnectorBeneluxGermany" class = "emlab.gen.domain.technology.Interconnector"
	p:capacity="${interconnectorCapacity}">
	<property name="connections">
		<set>
			<ref bean="beneluxNode" />
		</set>
	</property>
</bean>
	
<!-- Intermittent Production Profiles-->
<bean id = "windProfileNL" class = "emlab.gen.domain.technology.IntermittentResourceProfile"
	p:filename="/data/windProfileNL.csv" >
	<property name="intermittentTechnology" ref="windPGT"></property>
	<property name="intermittentProductionNode" ref="beneluxNode"></property>
</bean>
<bean id = "windProfileOffshoreNL" class = "emlab.gen.domain.technology.IntermittentResourceProfile"
	p:filename="/data/windProfileOffshoreNL.csv" >
	<property name="intermittentTechnology" ref="windOffshorePGT"></property>
	<property name="intermittentProductionNode" ref="beneluxNode"></property>
</bean>
<bean id = "sunProfileNL" class = "emlab.gen.domain.technology.IntermittentResourceProfile"
	p:filename="/data/sunProfileNL.csv" >
	<property name="intermittentTechnology" ref="photovolticPGT"></property>
	<property name="intermittentProductionNode" ref="beneluxNode"></property>
</bean>

<bean id = "demandNL" class = "emlab.gen.trend.HourlyCSVTimeSeries" p:filename="/data/demandNL2010.csv"/>

